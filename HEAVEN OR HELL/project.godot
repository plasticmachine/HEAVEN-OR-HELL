config_version=5

[application]

config/name="HEAVEN / HELL"
config/description="a little game about an angel and a devil finding eachother in the sun"
config/version=".3a"
run/main_scene="res://Scenes/main.tscn"
config/features=PackedStringArray("4.2", "Forward Plus")
run/max_fps=60
boot_splash/image="res://art/reimu/down/Untitled_Artwork-21.png"
config/icon="res://art/HEAVEN/down/downwalk_1.png"
scenes/loading_scene_path="res://addons/yet_another_scene_manager/Scenes/Loading.tscn"
scenes/error_scene_path="res://addons/yet_another_scene_manager/Scenes/Error.tscn"

[autoload]

Spawning="*res://addons/BulletUpHell/Spawning.tscn"
DialogueManager="*res://addons/dialogue_manager/dialogue_manager.gd"
Panku="*res://addons/panku_console/console.tscn"
YASM="*res://addons/yet_another_scene_manager/Scenes.gd"
StatSaver="*res://Scenes/stat_saver.tscn"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/mode=3
window/size/borderless=true
window/stretch/mode="canvas_items"
mouse_cursor/custom_image="res://art/misc/xjwmt5upomxa1-export.png"

[dotnet]

project/assembly_name="Reimu-s Bad Day"

[editor_plugins]

enabled=PackedStringArray("res://addons/BulletUpHell/plugin.cfg", "res://addons/dialogue_manager/plugin.cfg", "res://addons/panku_console/plugin.cfg", "res://addons/phantom_camera/plugin.cfg", "res://addons/yet_another_scene_manager/plugin.cfg")

